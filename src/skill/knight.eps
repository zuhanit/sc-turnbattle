import skill.skill;
import skill.detail;

var charSkillObject = 0;

function registerSkills() {
    const skill = skill.Skill.alloc();

    const qDetail = detail.Detail.alloc();
    qDetail.name = EPD(Db("베기"));
    qDetail.mp = 1;
    qDetail.description = EPD(Db("자신 기준 2X2칸 내의 적에게 20의 피해를 입힙니다."));

    const wDetail = detail.Detail.alloc();
    wDetail.name = EPD(Db("왕가의 방패"));
    wDetail.mp = 1;
    wDetail.description = EPD(Db("1턴간 쉴드를 30 획득합니다."));

    const eDetail = detail.Detail.alloc();
    eDetail.name = EPD(Db("기절"));
    eDetail.mp = 3;
    eDetail.description = EPD(Db("지정한 적을 1턴간 \x19기절\x04시킵니다."));

    const rDetail = detail.Detail.alloc();
    rDetail.name = EPD(Db("거합베기"));
    rDetail.mp = 2;
    rDetail.description = EPD(Db("1초간 정신을 집중한 뒤 지정한 적에게 달려들어 \x1170의 피해\x04를 입힙니다."));

    skill.q = qDetail;
    skill.w = wDetail;
    skill.e = eDetail;
    skill.r = rDetail;

    charSkillObject = skill;
}

