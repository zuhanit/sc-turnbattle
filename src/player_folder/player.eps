import ord.order;

object Player {
    var character;
    var hp;
    var mp;
    var epd;
    var firstAction: order.Action;
    var lastAction: order.Action;
};

const players = EUDArray(6);

function initialize() {
    for (var p = 0; p < 6; p++) {
        const pObj = Player.alloc();
        players[p] = pObj;
    }
}